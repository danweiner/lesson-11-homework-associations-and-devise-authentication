<h1>Headline News</h1>

  <h3><%= @article.title %></h3>
  <p><%= @article.body %></p>
  <p>Written by: <%= @article.author.try(:first_name) %> <%= @article.author.try(:last_name) %></p>
  <h2>Tags</h2> 
    <ul>   
      <% @article.tags.each do |tag| %>     
        <li><%= tag.name %></li>   
      <% end %> 
    </ul>


<%= link_to "Edit This Article", edit_article_path(@article) %> <br>
<%= link_to 'List of Articles', articles_path %>